<table>
  <thead>
    <tr>
      <th>User Id</th>
      <th>Email</th>
      <th>Role</th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |u| %>
      <tr>
        <td><%= u.id %></td>
        <td><%= u.email %></td>
        <td><%= u.role %></td>
      </tr>
    <% end %>
  </tbody>
</table>

